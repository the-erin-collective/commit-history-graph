{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@app/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "local.config.d.ts", "responseEvent.d.ts", "commit.d.ts", "local.settings.js", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "local.settings.sample.js"],
  "exclude": ["node_modules", "local.settings.sample.js"]
}
